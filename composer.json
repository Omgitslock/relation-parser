{
    "name": "omgitslock/relation-parser",
    "description": "Парсер отношений eloquent моделей для Laravel 5.5 и выше",
    "type": "library",
    "authors": [
        {
            "name": "Pavlusenko Danil",
            "email": "dan4554@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "^7.2",
        "illuminate/database": "5.5.*|5.6.*|5.7.*",
        "myclabs/php-enum": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omgitslock\\RelationParser\\Providers\\RelationParserServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Omgitslock\\RelationParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omgitslock\\RelationParser\\Test\\": "tests"
        }
    }
}
